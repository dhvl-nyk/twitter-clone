<h1>Login Page</h1>
<div class= "Sign_Form">
  <%= form_tag(:action => 'login') do %>
    <p>Username or Email:</br> <%= text_field_tag(:username_or_email) %></p> 
    <p>Password:</br> <%= password_field_tag :login_password %></p> 
	<p><%= link_to 'Sign Up', new_user_path %></p>
    <%= submit_tag("Log In") %>
  <% end %>
</div>
<p id="alert"><%= alert %></p>